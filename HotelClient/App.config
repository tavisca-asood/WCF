                binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IHotelOperations"
                contract="HotelServiceReference.IHotelOperations" name="WSHttpBinding_IHotelOperations">
                <identity>
                    <userPrincipalName value="asood@internal.tavisca.com" />
                </identity>
            </endpoint>
